				  <script type="text/javascript">
				    setTimeout("new Effect.Fade('notice');", 15000)
				  </script>
				<% end %>
				<%= yield %>
			</div>
			<div id="footer">
				<p>Föllöv ze veit rabbit!</p>
			</div>
		</div>
	</div>
</body>
</html>
