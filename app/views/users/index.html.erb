<img src="/images/titles/users.png" />

<%@users.each do |user| %>
	<div class="userfloat">
		<%= link_to(image_tag("/images/none.png"), user) %>
		<br/>
		<%= link_to user.alias, user %> 
		<%if session[:logged_in]%>
			<% if session[:user_alias] == user.alias%>
				<br/>
				<%= link_to "edit", edit_user_path(user) %>
			<%end%>
		<%end%>
	</div>
<% end %>